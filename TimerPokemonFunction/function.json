{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      // The schedule uses CRON syntax: second minute hour day month day-of-week.
      // The default here runs every 5 minutes.  You can customise this by
      // changing the value or overriding it in the function app settings.
      "schedule": "0 */5 * * * *"
    }
  ]
}